<h2>Check Out</h2>
<h5> <%= link_to "Home Page", '/' %></h5>
<%= link_to 'Figures', '/figures' %>
<%= link_to 'Factions', '/factions' %>
<%= link_to 'Contact Page', '/contact' %>
<%= link_to 'Shopping Cart', '/pages/cart' %>
<% total=0 %>
<% @cart.each do |f| %>
    <% @figure = Figure.find(f[0]) %>
    <% total += @figure.price*f[1] %>
   <div>
        <%= @figure.name %>
        $<%= @figure.price*f[1] %>
        Quantity: <%= f[1] %>
   </div>
<% end %>
Total: $<%= total %>